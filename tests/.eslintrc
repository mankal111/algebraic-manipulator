{
  "env": {
    "mocha": true
  },
  "extends": "airbnb",
  "plugins": ["react"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions":
          [".spec.js"]
      }
    ]
  }
}
